{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest --config ../testing/frontend/vitest.config.js ../testing/frontend",
    "test:ui": "vitest --ui --config ../testing/frontend/vitest.config.js ../testing/frontend",
    "test:coverage": "vitest --coverage --config ../testing/frontend/vitest.config.js ../testing/frontend",
    "cypress": "cypress open --config-file ../testing/e2e-cypress/cypress.config.js --project ../testing/e2e-cypress",
    "cypress:run": "cypress run --config-file ../testing/e2e-cypress/cypress.config.js --project ../testing/e2e-cypress",
    "test:e2e": "npm run cypress:run"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.4"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^7.0.0",
    "@eslint/js": "^9.36.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "cypress": "^15.5.0",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "jsdom": "^27.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "vite": "^7.1.7",
    "vitest": "^4.0.1"
  }
}
